{
    "default_commands": {
        "open_app": {
            "patterns": [
                [
                    "open"
                ],
                [
                    "initialise"
                ],
                [
                    "initialize"
                ]
            ],
            "start_responses": [
                "Opening  <app_name>",
                "Please wait, I am opening <app_name>"
            ],
            "stop_responses": [
                "<app_name> is open.",
                "Done opening <app_name>"
            ]
        },
        "close_app": {
            "patterns": [
                [
                    "close"
                ]
            ],
            "start_responses": [
                "Closing  <app_name>",
                "Please wait, I am closing <app_name>"
            ],
            "stop_responses": [
                "<app_name> is closed.",
                "Done closing <app_name>"
            ]
        },
        "create_new": {
            "patterns": [
                [
                    "new",
                    "document"
                ],
                [
                    "create",
                    "new"
                ],
                [
                    "create",
                    "new",
                    "document"
                ],
                [
                    "compose",
                    "new"
                ]
            ],
            "start_responses": [
                "Please wait."
            ],
            "stop_responses": [
                "Done!"
            ]
        },
        "save_as": {
            "patterns": [
                [
                    "save",
                    "document"
                ],
                [
                    "save"
                ]
            ],
            "start_responses": [
                "Please wait. I am saving!"
            ],
            "stop_responses": [
                "Done!",
                "Saved!",
                "<app_name> is saved."
            ]
        }
    }
}